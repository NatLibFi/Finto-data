yso-meta:Concept
    a owl:Class ;
    rdfs:label "YSO Concept"@en, "YSO-käsite"@fi, "YSO-begrepp"@sv ;
    rdfs:subClassOf skos:Concept .

yso-meta:singularPrefLabel a owl:DatatypeProperty ;
    rdfs:subPropertyOf skos:hiddenLabel ;
    rdfs:label "singular preferred label"@en, "käytettävän termin yksikkömuoto"@fi, "föredragen term i singular"@sv ;
    rdfs:domain yso-meta:Concept .

yso-meta:singularAltLabel a owl:DatatypeProperty ;
    rdfs:subPropertyOf skos:hiddenLabel ;
    rdfs:label "singular alternate label"@en, "vaihtoehtoisen termin yksikkömuoto"@fi, "alternativ term i singular"@sv ;
    rdfs:domain yso-meta:Concept .

rdau:P60683 a rdf:Property ;
    rdfs:label "edeltäjä"@fi, "företrädare"@sv, "has predecessor"@en .

rdau:P60686 a rdf:Property ;
    rdfs:label "seuraaja"@fi, "efterträdare"@sv, "has successor"@en .
