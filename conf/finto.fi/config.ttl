# Skosmos main configuration

:config a skosmos:Configuration ;
    # SPARQL endpoint
    # a local Fuseki server is usually on localhost:3030
    skosmos:sparqlEndpoint <http://api.finto.fi/sparql> ;
    # sparql-query extension, or "Generic" for plain SPARQL 1.1
    # set to "JenaText" instead if you use Fuseki with jena-text index
    skosmos:sparqlDialect "JenaText" ;
    # whether to enable collation in sparql queries
    skosmos:sparqlCollationEnabled true ;
    # HTTP client configuration
    # skosmos:sparqlTimeout ? ;
    skosmos:httpTimeout 5 ;
    # customize the service name
    skosmos:serviceName "Finto" ;
    skosmos:serviceNameLong "Ontologia- ja sanastopalvelu Finto"@fi ,
	"Finländsk ontologi- och vokabulärservice Finto"@sv ,
	"Finnish Ontology and Vocabulary Service Finto"@en ,
	"Suopmelaš ontologiija- ja sátnerádjobalválus Finto"@se ;
    skosmos:serviceDescription "Yhteentoimivat ontologia- ja sanastopalvelut tietoaineistojen kuvailuun"@fi ,
	"Samverkansfrämjande ontologi- och vokabulärtjänster för beskrivning av informationsmaterial"@sv ,
	"Interoperable vocabulary and ontology services for dataset indexing"@en ,
	"Ovttasdoaibmi ontologiija- ja sátnerádjobálvalusat diehtomateriálaid govvideapmái"@se ;
    skosmos:feedbackDescription "Anna palautetta ontologia- ja sanastopalvelu Finton sanastoista"@fi ,
	"Ge respons på vokabulärerna i Finto-servicen"@sv ,
	"Send vocabulary feedback to Finnish ontology and vocabulary service Finto"@en ,
	"Atte máhcahaga ontologiija- ja sátnerádjobálvalus Finto sátnerájuin"@se ;
    skosmos:aboutDescription "Lue lisää ontologia- ja sanastopalvelu Finton käyttäjistä ja hyödyistä"@fi ,
	"Läs mer om ontologi- och vokabulärservicen Fintos fördelar och användare"@sv ,
	"More information on ontology and vocabulary service Finto"@en ,
	"Loga lasi ontologiija- ja sátnerádjobálvalus Finto geavaheaddjiin ja ávkkiin"@se ;
    # customize the base element. Set this if the automatic base url detection doesn't work. For example setups behind a proxy.
    # skosmos:baseHref "?" ;
    # interface languages available, and the corresponding system locales
    skosmos:languages (
        [ rdfs:label "fi" ; rdf:value "fi_FI.utf8" ]
        [ rdfs:label "sv" ; rdf:value "sv_SE.utf8" ]
        [ rdfs:label "en" ; rdf:value "en_GB.utf8" ]
        [ rdfs:label "se" ; rdf:value "se_NO" ]
    ) ;
    # how many results (maximum) to load at a time on the search results page
    # skosmos:searchResultsSize ? ;
    # how many items (maximum) to retrieve in transitive property queries
    skosmos:transitiveLimit 1000 ;
    # whether or not to log caught exceptions
    skosmos:logCaughtExceptions false ;
    # set to TRUE to enable logging into browser console
    # skosmos:logBrowserConsole ? ;
    # set to a logfile path to enable logging into log file
    # skosmos:logFileName "?" ;
    # a default location for Twig template rendering
    skosmos:templateCache "/tmp/finto.fi-template-cache" ;
    # customize the css by adding your own stylesheet
    skosmos:customCss "resource/css/finto.css" ;
    # default email address where to send the feedback
    skosmos:feedbackAddress "finto-posti@helsinki.fi" ;
    # email address to set as the sender for feedback messages
    # skosmos:feedbackSender "?" ;
    # email address to set as the envelope sender for feedback messages
    skosmos:feedbackEnvelopeSender "" ;
    # whether to display the ui language selection as a dropdown (useful for cases where there are more than 3 languages)
    # skosmos:uiLanguageDropdown ? ;
    # whether to enable the spam honey pot or not, enabled by default
    # skosmos:uiHoneypotEnabled ? ;
    # default time a user must wait before submitting a form
    # skosmos:uiHoneypotTime ? ;
    # plugins to activate for the whole installation (including all vocabularies)
    skosmos:globalPlugins ( "yse" ) .

